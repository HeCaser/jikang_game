        <activity
            android:name=".activity.SpeedReadNumberActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SearchWordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ScoreRecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SearchNumberActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SearchOddEvenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SearchRecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.BeforeEBookActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.EBookTreeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>