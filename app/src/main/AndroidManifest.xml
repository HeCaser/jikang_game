        android:roundIcon="@mipmap/taikang_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.BeforeSearchActivity"></activity>
        <activity
            android:name=".activity.OneSharpEyeActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.OneToHundredResultActivity" />
        <activity
